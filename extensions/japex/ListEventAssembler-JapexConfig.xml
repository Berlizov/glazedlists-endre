<?xml version="1.0" encoding="UTF-8"?>
<testSuite name="ParsingPerformance" xmlns="http://www.sun.com/japex/testSuite">

    <param name="japex.classPath" value="${JAPEX_HOME}/jdsl/*.jar"/>
    <param name="japex.classPath" value="classes"/>

    <!-- Run each test for 20 seconds -->
    <param name="japex.warmupTime" value="10"/>
    <param name="japex.runTime" value="10"/>

    <driver name="List Event Blocks">
        <param name="japex.driverClass" value="com.publicobject.glazedlists.japex.BlockDeltasDriver"/>
        <param name="description" value="List Event Blocks"/>
    </driver>

    <driver name="List Event Deltas">
        <param name="japex.driverClass" value="com.publicobject.glazedlists.japex.BarcodeDeltasDriver"/>
        <param name="description" value="Barcode Deltas"/>
    </driver>

    <driver name="List Event Deltas 2">
        <param name="japex.driverClass" value="com.publicobject.glazedlists.japex.TreeDeltasDriver"/>
        <param name="description" value="Tree Deltas"/>
    </driver>

    <testCase name="List Event Performance">
    </testCase>

</testSuite>
